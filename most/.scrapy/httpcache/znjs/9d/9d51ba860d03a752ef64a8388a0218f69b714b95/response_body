<script>
function transearchword()
{
    
    var str = form1.searchword.value;
	if(str=="关键词检索"|str=="")
	{
    		alert("请输入检索关键词!");
		return false;
	}
	str=str.replace(/(^[\s\u3000]*)|([\s\u3000]*$)/g, "");
	str=str.replace(/(^\u3000*)|(\u3000*$)/g, "");
	str=str.replace(/(^\s*)|(\s*$)/g, "");
	var DBCStr="";
	var int=str.length;
	for(var i=0; i<int; i++)
	{ 	
		var c = str.charCodeAt(i);
		if(c == 12288|c == 32)
		{
		  if(DBCStr.charAt(DBCStr.length-1)!="*") 
		  {
		  	DBCStr+="*"; 
		  }
		}
		else
		{	
		  DBCStr+= String.fromCharCode(c); 
		}
	} 	
	form1.searchword.value=DBCStr;
    
}
function reloadparent(selectvalue)
{	
	if(document.body.scrollHeight>550)
	{
		if(parent.document.getElementById("main")!=null)
		{
			parent.document.getElementById("main").height=this.document.body.scrollHeight;
		}
	}	
	else
	{
		if(parent.document.getElementById("main")!=null)
		{
			parent.document.getElementById("main").height = 550;		
			document.getElementById("tab").height=550;
		}
	}


if(selectvalue == "+DOCRELTIME")
{
document.all.sortfield.selectedIndex=1;
}
if(selectvalue == "-DOCRELTIME")
{
document.all.sortfield.selectedIndex=0;
}	

if(27195 <= 6)
{
Zoom.style.display="none";
}
}

function getWord()
 {

	var txt = '';
	var foundIn = '';
	if (window.getSelection)
	{
		txt = window.getSelection();		
	}
	else if (document.getSelection)
	{
		txt = document.getSelection();	
	}
	else if (document.selection)
	{
		txt = document.selection.createRange().text;
	}
	
	if(txt!="")
	{
		if(typeof(document.getElementById("searchword"))!="undefined")
		{
			document.getElementById("searchword").value=txt;
		}	
	}
}
function checkpage()
{
	var all = 907;	
	if(document.page.page.value>all)
	{
		document.page.page.value = all;
	}
}
function checkpage2()
{
	var all = 907;	
	if(document.page2.page.value>all)
	{
		document.page2.page.value = all;
	}
}
